<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white" ng-app="ngApp" ng-init="NoDataForSave='@Token.NoDataForSave'">

    <div class="page-wrapper" ng-controller="ctrlLayOut">


        <div id="tecSupportModel" class="modal fade">
            <div class="modal-dialog">
                <div class="modal-content">
                    <!-- dialog body -->
                    <div class="modal-body">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <td class="th-20">@Token.UserName</td>
                                    <td class="th-20">@Token.Email</td>
                                    <td class="th-20">@Token.Subject</td>
                                    <td class="th-60">@Token.Message</td>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>{{tecSupport.UserName}}</td>
                                    <td>{{tecSupport.Email}}</td>
                                    <td>{{tecSupport.Subject}}</td>
                                    <td>
                                        <textarea class="form-control min-Width-100 input-disabled" rows="3" ng-model="tecSupport.Message" disabled></textarea>
                                    </td>
                                </tr>
                            </tbody>
                        </table>

                    </div>
                    <!-- dialog buttons -->
                    <div class="modal-footer"><button type="button" class="btn btn-primary" data-dismiss="modal">@Token.Cancel</button></div>
                </div>
            </div>
        </div>

        <div id="notifyModel" class="modal fade">
            <div class="modal-dialog">
                <div class="modal-content">
                    <!-- dialog body -->
                    <div class="modal-body">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <td class="th-20">@Token.Title</td>
                                    <td class="th-80">@Token.Message</td>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>{{notify.Title}}</td>
                                    <td>
                                        <textarea class="form-control min-Width-100 input-disabled" rows="3" ng-model="notify.Body" disabled></textarea>
                                    </td>
                                </tr>
                            </tbody>
                        </table>

                    </div>
                    <!-- dialog buttons -->
                    <div class="modal-footer"><button type="button" class="btn btn-primary" data-dismiss="modal">@Token.Cancel</button></div>
                </div>
            </div>
        </div>

        <!-- BEGIN HEADER -->
        <div class="page-header navbar navbar-fixed-top">
            <!-- BEGIN HEADER INNER -->
            <div class="page-header-inner ">
                <!-- BEGIN LOGO -->
                <div class="page-logo">
                    <a href="index.html">
                        <img src="/Files/Images/Logo.png" style="    width: 20px;" alt="logo" class="logo-default" />
                    </a>
                    <div class="menu-toggler sidebar-toggler">
                        <span></span>
                    </div>
                </div>
                <!-- END LOGO -->
                <!-- BEGIN RESPONSIVE MENU TOGGLER -->
                <a href="javascript:;" class="menu-toggler responsive-toggler" data-toggle="collapse" data-target=".navbar-collapse">
                    <span></span>
                </a>
                <!-- END RESPONSIVE MENU TOGGLER -->
                <!-- BEGIN TOP NAVIGATION MENU -->
                <div class="top-menu">
                    <ul class="nav navbar-nav pull-right">
                        <li class="dropdown dropdown-extended dropdown-notification" id="header_notification_bar">
                            <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
                                <i class="icon-bell"></i>
                                <span class="badge badge-default">{{Notifications.length}}   </span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="external">
                                    <h3>
                                        <span class="bold"> {{Notifications.length}} </span> @Token.Notifications @Token.From  {{NotificationCount}}
                                    </h3>
                                    <a href="~/Notifications">@Token.ViewAll</a>
                                </li>
                                <li>
                                    <ul class="dropdown-menu-list scroller" style="height: 250px;" data-handle-color="#637283">
                                        <li ng-repeat="x in Notifications" ng-click="readNotification(x)">
                                            <a>
                                                <div>
                                                    <span class="time">{{x.DateTimeSince}}</span>
                                                    <span class="details">
                                                        <span class="label label-sm label-icon label-success">
                                                            <i class="fa fa-bell"></i>
                                                        </span>
                                                    </span>
                                                    <span class="message">{{x.Title}}</span>

                                                </div>
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </li>

                        <li class="dropdown dropdown-extended dropdown-inbox" id="header_inbox_bar">
                            <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
                                <i class="icon-envelope-open"></i>
                                <span class="badge badge-default"> {{TechnicalSupport.length}} </span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="external">
                                    <h3>
                                        <span class="bold"></span>
                                        <span class="bold"> {{TechnicalSupport.length}} </span> @Token.TechnicalSupport @Token.From  {{TechnicalSupportCount}}
                                    </h3>
                                    <a href="/TechnicalSupport">@Token.ViewAll</a>
                                </li>
                                <li>
                                    <ul class="dropdown-menu-list scroller" style="height: 275px;" data-handle-color="#637283">
                                        <li ng-repeat="x in TechnicalSupport" ng-click="readTecSupport(x)">
                                            <a>
                                                <span class="photo">
                                                    <img src="~/Files/Images/images.png" class="img-circle" alt="">
                                                </span>
                                                <span class="subject">
                                                    <span class="from">{{x.Email}} </span>
                                                    <span class="time">{{x.DateTimeSince}} </span>
                                                </span>
                                                <span class="message">{{x.Message.substring(0,30)}}...</span>
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </li>



                        <!-- DOC: Apply "dropdown-dark" class after below "dropdown-extended" to change the dropdown styte -->
                        <li class="dropdown dropdown-user">
                            <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
                                <img alt="" class="img-circle" src="{{UserData.ImageUrl}}" />
                                <span class="username username-hide-on-mobile"> {{UserData.UserName}} </span>
                                <i class="fa fa-angle-down"></i>
                            </a>
                            <ul class="dropdown-menu dropdown-menu-default">
                                <li>
                                    <a href="~/UserProfile">
                                        <i class="icon-user"></i> @Token.UserProfile
                                    </a>
                                </li>
                                <li>
                                    <a href="Logout">
                                        <i class="icon-key"></i> @Token.Logout
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <!-- END USER LOGIN DROPDOWN -->
                        <!-- BEGIN QUICK SIDEBAR TOGGLER -->
                    </ul>
                </div>
                <!-- END TOP NAVIGATION MENU -->
            </div>
            <!-- END HEADER INNER -->
        </div>
        <!-- END HEADER -->
        <!-- BEGIN HEADER & CONTENT DIVIDER -->
        <div class="clearfix"> </div>
        <!-- END HEADER & CONTENT DIVIDER -->
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <!-- BEGIN SIDEBAR -->
            @Html.Partial("_Menu")
            <!-- END SIDEBAR -->
            <!-- BEGIN CONTENT -->
            <div class="page-content-wrapper">
                <!-- BEGIN CONTENT BODY -->
                <div class="page-content">
                    <!-- BEGIN PAGE HEADER-->
                    <!-- BEGIN THEME PANEL -->
                    <div class="theme-panel hidden-xs hidden-sm">
                        <div class="toggler"> </div>
                        <div class="toggler-close"> </div>

                    </div>
                    <!-- END THEME PANEL -->
                    <!-- BEGIN PAGE BAR -->
                    @if (IsSectionDefined("bar"))
                    {
                        @RenderSection("bar")

                    }
                    else
                    {
                        <div class="page-bar">
                            <ul class="page-breadcrumb">
                                <li>
                                    <a href="Home">@Token.Home</a>
                                    <i class="fa fa-circle"></i>
                                </li>
                                <li>
                                    <span> @ViewBag.Title</span>
                                </li>
                            </ul>
                            <div class="page-toolbar">
                                <div id="dashboard-report-range" class="pull-right tooltips btn btn-sm" data-container="body" data-placement="bottom" data-original-title="Change dashboard date range">
                                    <i class="icon-calendar"></i>&nbsp;
                                    <span class="thin uppercase hidden-xs"></span>&nbsp;
                                    <i class="fa fa-angle-down"></i>
                                </div>
                            </div>
                        </div>
                    }
                    <!-- END PAGE BAR -->
                    <!-- BEGIN PAGE TITLE-->
                    <h1 class="page-title">
                        @ViewBag.Title
                        <small> @ViewBag.Description</small>
                    </h1>
                    <!-- END PAGE TITLE-->
                    <!-- END PAGE HEADER-->
                    @RenderBody()
                </div>
                <!-- END CONTENT BODY -->
            </div>
            <!-- END CONTENT -->
